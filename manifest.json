{
  "manifest_version": 3,
  "name": "Search Commander",
  "version": "1.0",
  "description": "Custom search commands extension",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      }
    }
  }
}
